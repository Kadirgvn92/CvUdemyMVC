
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_DefaultHeaderComponentPartial")

<body id="page-top">
    <!-- Navigation-->
    @await Component.InvokeAsync("_DefaultNavbarComponentPartial")
    <!-- Page Content-->
    <div class="container-fluid p-0">
        <!-- About-->
        @await Component.InvokeAsync("_DefaultAboutComponentPartial")
        <hr class="m-0" />
        <!-- Experience-->
        @await Component.InvokeAsync("_DefaultExprienceComponentPartial")
        <hr class="m-0" />
        <!-- Education-->
        @await Component.InvokeAsync("_DefaultEducationComponentPartial")
        <hr class="m-0" />
        <!-- Skills-->
        @await Component.InvokeAsync("_DefaultSkillComponentPartial")
        <hr class="m-0" />
        <!-- Interests-->
        @await Component.InvokeAsync("_DefaultHobbyComponentPartial")
        <hr class="m-0" />
        <!-- Awards-->
        @await Component.InvokeAsync("_DefaultCertificationComponentPartial")

    </div>
    <!-- Bootstrap core JS-->
    @await Component.InvokeAsync("_DefaultScriptComponentPartial")
</body>
</html>
